{
    "version": "2017-02-28",
    "operation": "Query",
    "index": "pollId-index",
    "query": {
        "expression": "poll_id = :poll_id",
        "expressionValues": {
            ":poll_id": {
                "S": "$ctx.args.poll_id"
            }
        }
    },
    "limit": #if($context.arguments.limit) $context.arguments.limit #else 10 #end,
    "nextToken": #if($context.arguments.nextToken) "$context.arguments.nextToken" #else null #end
}